{
    "autoload": {
        "files": [
            "src/Bojler/functions/util.php",
            "src/Bojler/functions/discord_util.php"
        ],
        "psr-4": {
            "Bojler\\": "src/Bojler"
        }
    },
    "require": {
        "team-reflex/discord-php": "^10",
        "intervention/image": "^3.3",
        "symfony/dotenv": "^7.0",
        "php-ds/php-ds": "^1.5",
        "react/async": "^4.2"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "mockery/mockery": "^1.6",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "php -c . src/bot.php"
        ]
    }
}
